        {
            "name": "Philipp Busse",
            "email": "pmb@heise.de"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "guzzlehttp/guzzle": "3.x",
        "zendframework/zend-cache": "*",
        "zendframework/zend-config": "*",
        "zendframework/zend-json": "*"
    },
    "autoload": {
        "psr-4": { "Heise\\Shariff\\": "src" }
    }
}
